{
  "name": "@ltip/api",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset",
    "scripts:clean": "rm -f scripts/*.js scripts/*.js.map scripts/*.d.ts scripts/*.d.ts.map",
    "import:run": "pnpm run scripts:clean && tsx scripts/bulk-import.ts",
    "import:dry-run": "pnpm run scripts:clean && tsx scripts/bulk-import.ts --dry-run",
    "import:status": "tsx scripts/bulk-import.ts --status",
    "import:reset": "tsx scripts/bulk-import.ts --reset"
  },
  "dependencies": {
    "@ltip/shared": "workspace:*",
    "@prisma/client": "^6.2.0",
    "argon2": "^0.44.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "helmet": "^8.0.0",
    "ioredis": "^5.9.2",
    "jsonwebtoken": "^9.0.3",
    "passport": "^0.7.0",
    "passport-github2": "^0.1.12",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "pino": "^9.6.0",
    "pino-http": "^10.4.0",
    "ws": "^8.18.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@ltip/eslint-config": "workspace:*",
    "@ltip/tsconfig": "workspace:*",
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/ioredis": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^22.10.0",
    "@types/passport": "^1.0.17",
    "@types/passport-github2": "^1.2.9",
    "@types/passport-google-oauth20": "^2.0.17",
    "@types/passport-jwt": "^4.0.1",
    "@types/ws": "^8.5.13",
    "eslint": "^8.57.1",
    "pino-pretty": "^13.0.0",
    "prisma": "^6.2.0",
    "tsx": "^4.19.2",
    "typescript": "^5.4.0",
    "vitest": "^2.1.8"
  }
}
